@model Web.Models.RefillLineViewModel

<tr>
    @Html.HiddenFor(m => m.pId)
    <td><input type="checkbox" value=""></td>
    <td><strong>@Model.PatientName</strong></td>
    <td>@Model.MedecineName.ToLower()</td>
    <td class="td-action">@Html.ActionLink("Edit patient", "EditPatient", "Home", routeValues: new { id = Model.pId, returnUrl = "Index" }, htmlAttributes: new { @class = "btn btn-default btn-sm" })</td>
</tr>
